<Alloy>
	<View class="container">
		<View class="blank"></View>
		<View  id="dialogBox">
			<Label id="titleDialog"></Label>
			<View class="horizontalLayout">
				<Label id="currentLocation" class="leftLbl"></Label>
				<Switch id="currentSwt" value="false" />
			</View>
			<View class="horizontalLayout">
				<Label id="homeLocation" class="leftLbl"></Label>
				<Switch id="homeSwt" value="false" />
			</View>
			<View>
				<Label class="optionsLeft" onClick="cancelWindow">Cancel</Label>
				<Label class="optionsRight" onClick="sendInfo">Ok</Label>
			</View>
		</View>
	</View>
</Alloy>